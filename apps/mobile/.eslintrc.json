{
	"extends": ["plugin:@nx/react", "../../.eslintrc.json"],
	"ignorePatterns": ["!**/*", "public", ".cache", "node_modules"],
	"overrides": [
		{
			"files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
			"rules": {
        "no-empty-function": "off",
        "@typescript-eslint/no-empty-function": "warn"
      }
		},
		{
			"files": ["*.ts", "*.tsx"],
			"rules": {}
		},
		{
			"files": ["*.js", "*.jsx"],
			"rules": {}
		}
	]
}
